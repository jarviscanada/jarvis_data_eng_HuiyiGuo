class Solution {
    public int countPrimes(int n) {
        boolean[] numPrimes = new boolean[n];
        int count = 0;
        for (int i=2; i < n; i++){
            if (numPrimes[i] == false){
                count++;
                for (int j=2; j*i < n; j++){
                    numPrimes[i*j] = true;
                }
            }
            
        }
        return count;
        
    }
}
